# Production Environment Variables for Railway
NODE_ENV=production
PORT=3001

# MongoDB Configuration (required)
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/arcadeconnect

# JWT Configuration (required)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-min-32-chars

# CORS Configuration (will be set automatically by Railway)
CLIENT_ORIGIN=https://your-app-name.railway.app
